@model IEnumerable<Its.Systems.HR.Domain.Model.HrPerson>

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Välkommen</h1>
    <p class="lead">Till HR-appen Kompetensutveckling.</p>
</div>

<div class="row">
    <div class="col-md-6">
        <h2>TEST HrPersonal:</h2>
        <ul>
            @foreach (var hrPerson in Model)
            {
                <li>@hrPerson.GetHrFullName()</li>
            }
        </ul>
    </div>
    <div class="col-md-6">
        <h2>Mer info</h2>
        <p>Inget här.</p>
    </div>
</div>