@if (Request.IsAuthenticated)
{
    <text>
        <ul class="nav navbar-nav navbar-right">
            <li class="navbar-text">
                @User.Identity.Name
            </li>
            <li>
                @Html.ActionLink("Logga ut", "SignOut", "Account")
            </li>
        </ul>
    </text>
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Logga in", "SignIn", "Account", null, new { id = "loginLink" })</li>
    </ul>
}